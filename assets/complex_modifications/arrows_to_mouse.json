{
  "title": "disable pageup pagedown home",
  "author": "Yang",
  "rules": [
    {
      "description": "disable pageup pagedown home",
      "manipulators": [
        {
          "from": {
            "key_code": "page_up",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "delete_or_backspace"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "page_down",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "backslash"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "home",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "return_or_enter"
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}
