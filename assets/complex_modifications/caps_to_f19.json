{
  "title": "caps_lock => f19 | ctrl",
  "author": "Vonng(fengruohang@outlook.com)",
  "rules": [
    {
      "description": "caps_lock =>  f19 | ctrl",
      "manipulators": [
        {
          "from": {
            "key_code": 57,
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "left_control",
              "repeat": true
            }
          ],
          "to_if_alone": [
            {
              "key_code": "f19"
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}
